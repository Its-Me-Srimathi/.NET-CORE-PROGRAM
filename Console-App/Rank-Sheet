string StudentId;
string StudentName;
float English;
float Tamil;
float Maths;
float Science;
float Social;

Console.WriteLine("Enter Student Id: ");
StudentId = Console.ReadLine();

Console.WriteLine("Enter Student Name: ");
StudentName = Console.ReadLine();

Console.WriteLine("Enter English mark: ");
English = float.Parse(Console.ReadLine());

Console.WriteLine("Enter Tamil mark: ");
Tamil = float.Parse(Console.ReadLine());

Console.WriteLine("Enter Maths mark: ");
Maths = float.Parse(Console.ReadLine());

Console.WriteLine("Enter Science mark: ");
Science = float.Parse(Console.ReadLine());

Console.WriteLine("Enter Social mark: ");
Social = float.Parse(Console.ReadLine());

Console.WriteLine("RANK SHEET");
Console.WriteLine("CLASS 5");
Console.WriteLine();
Console.WriteLine($"Student Id :  {StudentId}\nStudent Name : {StudentName}\n");
Console.WriteLine();
Console.WriteLine("MARKS SCORED");
Console.WriteLine();
Console.WriteLine($"ENGLISH :  {English}\nTAMIL :{Tamil}\nMATHS :{Maths}\nSCIENCE :{Science}\nSOCIAL :{Social}");

float Average = (English + Tamil + Maths + Science + Social) / 5;
Console.WriteLine();

if (Average > 90) {
    Console.WriteLine("Result : Excellent");
}
else if (Average > 80 || Average < 91)
{
    Console.WriteLine("Result : Very Good");
}
else if (Average > 70 || Average < 81)
{
    Console.WriteLine("Result : Very Good");
}
else if (Average > 60 || Average < 71)
{
    Console.WriteLine("Result : Very Good");
}
else if (Average > 50 || Average < 61)
{
    Console.WriteLine("Result : Very Good");
}
else if (Average > 40 || Average < 51)
{
    Console.WriteLine("Result : Very Good");
}
else
{
    Console.WriteLine("Result : Failed");
}



